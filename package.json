{
  "name": "gitbook-plugin-localized-footer-pdf",
  "version": "0.0.1",
  "engines": {
    "gitbook": ">=3.1.0"
  },
  "gitbook": {
    "properties": {
      "hline": {
        "type": "boolean",
        "default": "true",
        "description": "whether to include an horizontal line above the footer"
      },
      "filename": {
        "type": "string",
        "default": "FOOTER.md",
        "description": "name or relative path to the footer content file of each book"
      }
    }
  },
  "description": "add a footer to each page of a gitbook, allows i10n",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "ryanjfrizzell",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/noerw/gitbook-plugin-localized-footer-pdf.git"
  },
  "bugs": {
    "url": "https://github.com/noerw/gitbook-plugin-localized-footer-pdf/issues"
  },
  "homepage": "https://github.com/noerw/gitbook-plugin-localized-footer-pdf",
